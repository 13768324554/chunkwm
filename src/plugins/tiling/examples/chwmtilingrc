#!/bin/bash

# Use 'chunkc' to send message to socket
export CHUNKC_SOCKET=4131

# if chunkc is not in path
chunkc=/usr/local/bin/chunkc

$chunkc global_virtual_space_mode float

$chunkc global_virtual_space_offset_top 200
$chunkc global_virtual_space_offset_bottom 190
$chunkc global_virtual_space_offset_left 190
$chunkc global_virtual_space_offset_right 190

$chunkc bsp_spawn_left 0
